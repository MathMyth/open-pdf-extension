{
    "manifest_version": 3,
    "name": "PDF Viewer Extension",
    "version": "1.0",
    "description": "Open Selected PDFs in Chrome",
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "permissions": [
      "contextMenus",
      "tabs"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "host_permissions": [
      "<all_urls>"
    ]
  }
  